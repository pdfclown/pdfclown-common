<!--
  Toolchains sample file.

  Copy the content of this file to `${user.home}/.m2/toolchains.xml` and customize it to your
  environment (see `jdkHome` element here below).

  See <https://maven.apache.org/guides/mini/guide-using-toolchains.html> for further information.
-->
<toolchains>
  <toolchain>
    <type>jdk</type>
    <provides>
      <version>11</version>
      <vendor>openjdk</vendor>
    </provides>
    <configuration>
      <!-- TODO: replace with your actual JDK 11 installation path -->
      <jdkHome>/usr/lib/jvm/java-1.11.0-openjdk-amd64</jdkHome>
    </configuration>
  </toolchain>
</toolchains>
